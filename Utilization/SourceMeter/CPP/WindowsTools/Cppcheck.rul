  <Metric id = "CPPCHECK_CAI">
    <Configuration name = "Default">
      <Enabled>true</Enabled>
      <Group>false</Group>
      <OriginalId>charArrayIndex</OriginalId>
      <GroupMember>Simple Type Rules</GroupMember>
      <Language lang = "eng">
        <Warning>true</Warning>
        <DisplayName>Char Array Index</DisplayName>
        <Description>&lt;p&gt;Signed ‘char’ type used as array index. If the value can be greater than 127 there will be a buffer underflow because of sign extension.&lt;/p&gt;&#x0A;</Description>
        <HelpText>&lt;p&gt;Signed ‘char’ type used as array index. If the value can be greater than 127 there will be a buffer underflow because of sign extension.&lt;/p&gt;&#x0A;</HelpText>
